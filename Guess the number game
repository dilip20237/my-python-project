import random
num = random.randint(1,20)
guess = int(input("can you guess the guess the number i am thinking? its less than a 20: "))
while num != guess:
    if guess>num:
        print("your number is higher")
    else:
        print("your number is lower")
    guess=int(input("guess again "))
    
print("you won!")
        
